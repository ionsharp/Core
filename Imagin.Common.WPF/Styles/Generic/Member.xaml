        <local:KeyTemplate DataType="{x:Type local:MemberModel}" DataKey="{x:Type Media:StringColor}">
            <ContentPresenter Content="{Binding}" ContentTemplate="{DynamicResource {x:Static local:PropertyGrid.ColorTemplateKey}}"/>
        </local:KeyTemplate>

