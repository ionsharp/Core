                    <Behavior:ManualBindingBehavior Property="{x:Static local:GradientBox.ValueProperty}" 
                        Mode="TwoWay" Path="Value" Source="{Binding}" UpdateSourceTrigger="{Binding UpdateSourceTrigger}" />
                </e:Interaction.Behaviors>
