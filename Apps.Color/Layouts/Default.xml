<?xml version="1.0" encoding="utf-8"?>
<Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="Imagin.Common.Controls">
  <Root xsi:type="DockLayoutGroup" Orientation="Horizontal">
    <Elements>
      <Element xsi:type="DockLayoutGroup" Orientation="Vertical" Width="60*">
        <Elements>
          <Element xsi:type="DockLayoutDocumentGroup" Height="75*"/>
          <Element xsi:type="DockLayoutPanelGroup" Height="25*">
            <Panels>
              <DockLayoutPanel IsSelected="true" Name="LogPanel"/>
            </Panels>
          </Element>
        </Elements>
      </Element>
      <Element xsi:type="DockLayoutPanelGroup" Width="5*">
        <Panels>
          <DockLayoutPanel IsSelected="true" Name="ComponentPanel"/>
        </Panels>
      </Element>
      <Element xsi:type="DockLayoutPanelGroup" Width="5*">
        <Panels>
          <DockLayoutPanel IsSelected="true" Name="AlphaPanel"/>
        </Panels>
      </Element>
      <Element xsi:type="DockLayoutPanelGroup" Width="30*">
        <Panels>
          <DockLayoutPanel IsSelected="true" Name="PropertiesPanel"/>
          <DockLayoutPanel Name="ColorsPanel"/>
          <DockLayoutPanel Name="OptionsPanel"/>
        </Panels>
      </Element>
    </Elements>
  </Root>
</Layout>